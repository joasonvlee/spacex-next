{
  "manifest_version": 2,
  "name": "SpaceX Next",
  "version": "1.0.0",
  "author": "Joas Onvlee",

  "description": "A Firefox addon displaying data about the next SpaceX launch.",
  "homepage_url": "https://github.com/joasonvlee/spacex-next",
  "icons": {
    "32": "img/logo-32.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "img/logo-32.png",
    "default_title": "SpaceX Next",
    "default_popup": "spacexnext.html"
  }
}